#include<stdio.h>

int main()

{

  int n,key,i;

  int A[n];

  printf("Enter the number of elements of array\n");

  scanf("%d",&n);

  printf("Enter the array elements\n");

  for (i=0;i<n;i++)

  {

    scanf("%d",&A[i]);

  }

  printf("Enter the value to be searched:");

  scanf("%d",&key);

  for (i=0;i<n;i++)

  {

    if(A[i]==key)

    {

      printf("index of key is %d",i);

      break;

    }

  }

  return 0;

}
